# MISTRAL AI Configuration (Free tier)
MISTRAL_API_KEY=your_mistral_api_key_here
MISTRAL_MODEL=mistral-small

# LangSmith Configuration (Free tier for debugging)
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_PROJECT=multimodal-audit-engine
LANGSMITH_ENDPOINT=https://api.smith.langchain.com

# FastAPI Server Configuration
FASTAPI_HOST=0.0.0.0
FASTAPI_PORT=8000
FASTAPI_RELOAD=true

# Application Configuration
ENVIRONMENT=development
LOG_LEVEL=INFO

# Video Processing
VIDEO_TEMP_DIR=./temp_videos
MAX_VIDEO_SIZE_MB=500

# Content Analysis
BATCH_SIZE=10
COMPLIANCE_THRESHOLD=0.8

# AWS (Deferred for next phase)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1

# OpenTelemetry (Deferred for production)
OTEL_ENABLED=false
